{
	"name": "campus-shelter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"check-types": "tsc --noEmit",
		"db:generate": "prisma generate",
		"db:pull": "prisma db pull",
		"db:push": "prisma db push",
		"dev": "next dev",
		"env:dev": "vercel env pull --environment=development && ln -sf .env.local .env",
		"env:preview": "vercel env pull --environment=preview && ln -sf .env.local .env",
		"env:prod": "vercel env pull --environment=production && ln -sf .env.local .env",
		"format": "biome format . --write",
		"lint": "lint-staged --allow-empty",
		"lint:fix": "bun lint --apply",
		"postinstall": "prisma generate",
		"prepare": "husky",
		"preinstall": "npx only-allow bun",
		"start": "next start",
		"studio": "dotenv -e .env.local npx prisma studio"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.4.0",
		"@prisma/client": "^7.4.0",
		"@t3-oss/env-nextjs": "^0.13.10",
		"bcrypt": "^6.0.0",
		"jsonwebtoken": "^9.0.3",
		"next": "16.1.6",
		"next-swagger-doc": "^0.4.1",
		"pg": "^8.18.0",
		"prisma": "^7.4.0",
		"swagger-ui-react": "^5.31.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.15",
		"@types/bcrypt": "^6.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^20",
		"@types/pg": "^8.16.0",
		"@types/react": "^19",
		"@types/swagger-ui-react": "^5.18.0",
		"dotenv-cli": "^11.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"openapi-types": "^12.1.3",
		"typescript": "^5"
	},
	"lint-staged": {
		"*.{js,mjs,jsx,ts,tsx,json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
